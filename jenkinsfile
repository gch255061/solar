pipeline {
  agent any

  tools {
    nodejs 'node18'
  }

  stages {
    stage('VM Node Version') {
      steps {
        sh '''
          which node
          which npm
          node -v
          npm -v
        '''
      }
    }

    stage('Install Dependencies') {
      steps {
        sh 'npm install'
      }
    }
  }
}
